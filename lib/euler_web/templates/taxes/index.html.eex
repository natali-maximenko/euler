<label>Введите ИНН</label>
<input id="itn-input" type="text"/>
<button id="send-button" class="btn btn-primary" type="submit">Отправить</button>
<div id="messages">
<%= for l <- @list do %>
<p>[<%= l.inserted_at |> to_string() |> String.slice(0..-9) %>] <%= l.itn %> : <%= l.valid %></p>
<% end %>
</div>